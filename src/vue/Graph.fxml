<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controller.GraphController">
   <children>
      <BarChart fx:id="mChart">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxes" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxes" side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
